<div class="row">
    <div class="col">
        {{ form_row(form.name) }}
    </div>
    <div class="col">
        <div style="height: 90px; width:90px; float: left">
            {% for row in game.generatorConfig.format %}
                <div style="height: 30px; width:90px; float: left">
                    {% for field in row %}
                        <div style="width:30px;height:30px; float:left">{{ field }}</div>
                    {% endfor %}
                </div>
            {% endfor %}
        </div>
        {{ form_row(form.fields) }}
    </div>
</div>